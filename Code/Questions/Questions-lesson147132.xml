            <response>Yes.</response>
            <answerformat>0</answerformat>
            <responseformat>0</responseformat>
            <attempts>
            </attempts>
          </answer>
          <answer id="21645">
            <jumpto>-80</jumpto>
            <grade>0</grade>
            <score>0</score>
            <flags>0</flags>
            <timecreated>1468090856</timecreated>
            <timemodified>0</timemodified>
            <answer_text>A push due to your foot.</answer_text>
            <response>No.  We are thinking about forces acting on the box after it has left contact with your foot.  The only way you could exert a force on the box after it leaves contact with your foot would be if you were a Jedi.</response>
            <answerformat>0</answerformat>
            <responseformat>0</responseformat>

            <response>No.  The momentum is only conserved in an isolated system.</response>
            <answerformat>0</answerformat>
            <responseformat>0</responseformat>
            <attempts>
            </attempts>
          </answer>
          <answer id="21924">
            <jumpto>-80</jumpto>
            <grade>0</grade>
            <score>0</score>
            <flags>0</flags>
            <timecreated>1471809101</timecreated>
            <timemodified>0</timemodified>

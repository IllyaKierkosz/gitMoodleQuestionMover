        <answers>
          <answer id="21917">
            <jumpto>9592</jumpto>
            <grade>0</grade>
            <score>1</score>
            <flags>0</flags>
            <timecreated>1471808689</timecreated>
            <timemodified>0</timemodified>
            <answer_text>A collision in which mechanical energy is conserved.</answer_text>
            <response>Yes.</response>
            <answerformat>0</answerformat>
            <responseformat>0</responseformat>
            <attempts>
            </attempts>
          </answer>
          <answer id="21918">
            <jumpto>-80</jumpto>
            <grade>0</grade>
            <score>0</score>
            <flags>0</flags>
            <timecreated>1471808689</timecreated>
            <timemodified>0</timemodified>
